    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch index.html (Chrome)",
            "request": "launch",
            "type": "chrome",
            "url": "${workspaceFolder}/index.html",
        },
        {
            "name": "Launch index.html (Firefox)",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "file": "${workspaceFolder}/index.html"
        }
    ]
}
